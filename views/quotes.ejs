<!DOCTYPE html>
<html>

<head>
  <meta charset='utf-8'>
  <link rel="icon" href="./favicon.ico" type="image/x-icon">
  <link rel='stylesheet' href='/styles.css'>
  <!-- Bootstrap minified CSS -->
  <link rel='stylesheet' href='bootstrap.min.css'>
  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Oleo+Script|Roboto" rel="stylesheet">
  <% if(user.email == 'mjenkins@ixl.com'){ %>
  <title>CS3K</title>
  <% } else {%>
  <title>Fetch - Quotes</title>
  <% } %>
</head>

<body>
  <%- include include/navbar %>
  <%- include include/header %>
  <%- include include/manageTemplatesModal %>
  <%- include include/manageLicensesModal %>
  <%- include include/addLicenseModal %>
  <div class='center'>
    <h3 id="tableTitle">Licenses</h3>
    <div class='col-lg-12 col-md-12 col-sm-12'>
      <div class="licensesTableLabels">
        <div class="licenseLabels">
          <div class="header">
            Type
          </div>
          <div class="header">
            Country Code
          </div>
          <div class="header">
            Subjects
          </div>
          <div class="header">
            Licenses
          </div>
          <div class="header">
            Cost
          </div>
          <div class="header">
            Rebate ID
          </div>
          <div class="header">
            Rebate Cost
          </div>
          <div class="header">
            Spanish Cost
          </div>
          <div class="header">
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class='center'>
    <div class='col-lg-12 col-md-12 col-sm-12'>
      <div class="licensesTable">
        <% for(var k = 0; k < licenses.length; k++ ) { %>
        <div class="license" id="<%= licenses[k].id %>">
          <span hidden id="currencySymbol">
            <%= licenses[k].currencySymbol; %></span>
          <div class="<%=  licenses[k].countryCode.toLowerCase(); %>" id="type">
            <%=  licenses[k].type %>
          </div>
          <div class="<%=  licenses[k].countryCode.toLowerCase(); %>" id="countryCode">
            <%=  licenses[k].countryCode; %>
          </div>
          <div class="<%=  licenses[k].countryCode.toLowerCase(); %>" id="subjectQuantity">
            <%=  licenses[k].subjectQuantity; %>
          </div>
          <div class="<%=  licenses[k].countryCode.toLowerCase(); %>" id="numberLicenses">
            <%=  licenses[k].numberLicenses; %>
          </div>
          <div class="<%=  licenses[k].countryCode.toLowerCase(); %>" id="cost">
            <%= licenses[k].currencySymbol; %>
            <%= licenses[k].cost; %>
          </div>
          <div class="<%=  licenses[k].countryCode.toLowerCase(); %>" id="rebateId">
            <%= licenses[k].rebateId  %>
          </div>
          <div class="<%=  licenses[k].countryCode.toLowerCase(); %>" id="rebateCost">
            <%= licenses[k].currencySymbol; %>
            <%= licenses[k].rebateCost  %>
          </div>
          <div class="<%=  licenses[k].countryCode.toLowerCase(); %>" id="spanishCost">
            <%= licenses[k].currencySymbol; %>
            <%= licenses[k].spanishCost  %>
          </div>
          <div>
            <% if(user.type == "admin"){ %>
            <a class="<%=  licenses[k].countryCode.toLowerCase(); %>" href='#manageLicenses' data-toggle='modal' data-target='#manageLicenses' id="<%= `x` + licenses[k].id %>" onclick="loadLicenseFields(event)">Edit</a>
            <% } %>
          </div>
        </div>
        <% } %>
      </div>
    </div>

    <% if(user.type == "admin"){ %>
    <a href='#addLicense' data-toggle='modal' data-target='#addLicense' id="addLicenseBtn">&#43; Add a license</a>
    <% } %>
  </div>
  <!-- Spacing -->
  <div class='row space'>
    <div class='col-lg-3'></div>
    <div class='col-lg-6'>
      <div>
        <br>
        <br>
      </div>
    </div>
    <div class='col-lg-3'></div>
  </div>
  <!-- End spacing -->

</body>
<script src='tinymce/tinymce.min.js'></script>
<script src='jquery.min.js'></script>
<script src='app-main.js'></script>
<script src='bootstrap.min.js'></script>
<script>
  var licenseValuesObject = {};
  var elementIds = [];
  var databaseId = 0;
</script>

</html>